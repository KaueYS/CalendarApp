@model IEnumerable<ClinicaWEBTempt38.Models.Procedimento>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProcedimentoNome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProcedimentoPreco)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Criado)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ProcedimentoNome)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ProcedimentoPreco)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Criado)
                </td>

                <td>
                    <a asp-action="Edit" asp-route-id="@item.ProcedimentoId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.ProcedimentoId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.ProcedimentoId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
